{
  "name": "vite-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "analyze": "npx vite-bundle-visualizer -o ./analyze.html",
    "dev": "vite --mode development",
    "build": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "deploy:dev": "scripts/second-deploy.sh dev",
    "deploy:prod": "scripts/second-deploy.sh prod",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prod": "vite --mode production",
    "commit": "cz"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@tanstack/react-query": "^5.61.4",
    "axios": "^1.7.8",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "fbemitter": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.53.2",
    "react-icons": "^5.3.0",
    "react-router-dom": "^7.0.1",
    "tailwind-merge": "^2.5.5",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.1",
    "@prdev-solutions/eslint-config": "^1.0.5",
    "@types/fbemitter": "^2.0.35",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "aws-sdk": "^2.1692.0",
    "commitizen": "^4.3.1",
    "eslint": "^8.57.1",
    "postcss": "^8.4.49",
    "rollup-plugin-analyzer": "^4.0.0",
    "s3-deploy": "^1.4.0",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2",
    "vite": "^6.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-svgr": "^4.3.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
